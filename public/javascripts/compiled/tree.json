{
  "Wrongler": {
    "desc": "Basic script",
    "requires": [

    ],
    "provides": [
      "Wrongler"
    ]
  },
  "Widget": {
    "desc": "Basic widget",
    "requires": [
      "Wrongler",
      "LSD/LSD.Type"
    ],
    "provides": [
      "Wrongler.Widget"
    ],
    "Input": {
      "desc": "A base class for all kinds of form controls",
      "requires": [
        "Wrongler.Widget",
        "Native/LSD.Native.Input.*",
        "LSD/LSD.Mixin.Placeholder"
      ],
      "provides": [
        "Wrongler.Widget.Input"
      ],
      "Range": {
        "desc": "Range slider input",
        "requires": [
          "Wrongler.Widget.Input",
          "Widgets/LSD.Widget.Input.Range"
        ],
        "provides": [
          "Wrongler.Widget.Input.Range"
        ]
      },
      "Radio": {
        "desc": "A radio button, set of connected widgets that steal checkedness from each other",
        "requires": [
          "Wrongler.Widget.Input",
          "Widgets/LSD.Widget.Input.Radio"
        ],
        "provides": [
          "Wrongler.Widget.Input.Radio"
        ]
      },
      "Checkbox": {
        "desc": "Boolean checkbox type of input",
        "requires": [
          "Wrongler.Widget.Input",
          "Widgets/LSD.Widget.Input.Checkbox"
        ],
        "provides": [
          "Wrongler.Widget.Input.Checkbox"
        ]
      }
    },
    "Animated": {
      "desc": "Basic widget animated",
      "requires": [
        "Wrongler.Widget",
        "LSD/LSD.Native",
        "LSD/LSD.Mixin.Animation",
        "LSD/LSD.Mixin.Focus",
        "Core/Fx.Transitions"
      ],
      "provides": [
        "Wrongler.Widget.Animated"
      ]
    },
    "Body": {
      "desc": "Body widget",
      "requires": [
        "Wrongler.Widget",
        "Native/LSD.Native.Body"
      ],
      "provides": [
        "Wrongler.Widget.Body"
      ],
      "Dialog": {
        "desc": "An in-page independent document (like iphone app page)",
        "requires": [
          "Wrongler.Widget.Body",
          "Native/LSD.Native.Body",
          "LSD/LSD.Trait.Fieldset"
        ],
        "provides": [
          "Wrongler.Widget.Body.Dialog"
        ]
      },
      "Dialog.Lightbox": {
        "desc": "An in-page independent document (like iphone app page)",
        "requires": [
          "Wrongler.Widget.Body.Dialog",
          "LSD/LSD.Mixin.Focus"
        ],
        "provides": [
          "Wrongler.Widget.Body.Dialog.Lightbox"
        ]
      }
    }
  },
  "Application": {
    "desc": "Main script of application",
    "requires": [
      "Wrongler",
      "Wrongler.Widget.*",
      "Native/LSD.Native.Form",
      "Native/LSD.Native.Textarea",
      "Native/LSD.Native.Button",
      "Native/LSD.Native.Label",
      "Widgets/LSD.Widget.Select",
      "LSD/LSD.Action.*",
      "LSD/LSD.Application"
    ],
    "provides": [
      "Wrongler.Application"
    ]
  }
}